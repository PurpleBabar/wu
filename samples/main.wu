extern module love
extern print :: (a: string) -> {}

struct point {
  x: float
  y: float
}

position := point {
  x: 100
  y: 100
}

love update = (dt: float) -> {
  position x += dt * 10
  position y += dt * 10 * 1.5
}

love draw = () -> love graphics rectangle("fill", position x, position y, 200, 200)
